package br.com.infortec.bean;

/**
 *
 * @author Israelly
 */
public class Unidade {
    
    int cod;
    String descricao;
    int qtde;

    public Unidade() {
    }

    public Unidade(String descricao, int qtde) {
        this.descricao = descricao;
        this.qtde = qtde;
    }

    public Unidade(int cod, String descricao, int qtde) {
        this.cod = cod;
        this.descricao = descricao;
        this.qtde = qtde;
    }

    public int getCod() {
        return cod;
    }

    public void setCod(int cod) {
        this.cod = cod;
    }

    public String getDescricao() {
        return descricao;
    }

    public void setDescricao(String descricao) {
        this.descricao = descricao;
    }

    public int getQtde() {
        return qtde;
    }

    public void setQtde(int qtde) {
        this.qtde = qtde;
    }
    
    
    
}
