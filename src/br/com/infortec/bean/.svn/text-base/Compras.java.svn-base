package br.com.infortec.bean;

/**
 *
 * @author Israelly
 */
public class Compras {
    
    int cod;
    double custoTotal;
    double vlrDesconto;
    double custoPgmt;
    Fornecedor fornecedor;

    public Compras() {
    }

    public Compras(Fornecedor fornecedor) {
        this.fornecedor = fornecedor;
    }

    public Compras(double custoTotal, double vlrDesconto, double custoPgmt, Fornecedor fornecedor) {
        this.custoTotal = custoTotal;
        this.vlrDesconto = vlrDesconto;
        this.custoPgmt = custoPgmt;
        this.fornecedor = fornecedor;
    }

    public Compras(int cod, double custoTotal, double vlrDesconto, double custoPgmt, Fornecedor fornecedor) {
        this.cod = cod;
        this.custoTotal = custoTotal;
        this.vlrDesconto = vlrDesconto;
        this.custoPgmt = custoPgmt;
        this.fornecedor = fornecedor;
    }

    public int getCod() {
        return cod;
    }

    public void setCod(int cod) {
        this.cod = cod;
    }

    public double getCustoPgmt() {
        return custoPgmt;
    }

    public void setCustoPgmt(double custoPgmt) {
        this.custoPgmt = custoPgmt;
    }

    public double getCustoTotal() {
        return custoTotal;
    }

    public void setCustoTotal(double custoTotal) {
        this.custoTotal = custoTotal;
    }

    public Fornecedor getFornecedor() {
        return fornecedor;
    }

    public void setFornecedor(Fornecedor fornecedor) {
        this.fornecedor = fornecedor;
    }

    public double getVlrDesconto() {
        return vlrDesconto;
    }

    public void setVlrDesconto(double vlrDesconto) {
        this.vlrDesconto = vlrDesconto;
    }
  
    
}

