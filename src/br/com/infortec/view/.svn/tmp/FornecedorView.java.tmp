private void tipoComBoxItemStateChanged(java.awt.event.ItemEvent evt) {//GEN-FIRST:event_tipoComBoxItemStateChanged
    if ("Física".equals((String) tipoComBox.getSelectedItem())) {//pessoa física
        cnpjFornecedorField.setVisible(false);
        cnpjFornecedorLabel.setVisible(false);
        cpfLabel.setVisible(true);
        cpfFornecedorFileld.setVisible(true);

    } else if ("Jurídica".equals((String) tipoComBox.getSelectedItem())) {//pessoa jurídica
        cnpjFornecedorField.setVisible(true);
        cnpjFornecedorLabel.setVisible(true);
        cpfLabel.setVisible(false);
        cpfFornecedorFileld.setVisible(false);

    } else if ("".equals((String) tipoComBox.getSelectedItem())) {
        cnpjFornecedorField.setVisible(false);
        cnpjFornecedorLabel.setVisible(false);
        cpfLabel.setVisible(false);
        cpfFornecedorFileld.setVisible(false);
    }
}//GEN-LAST:event_tipoComBoxItemStateChanged
