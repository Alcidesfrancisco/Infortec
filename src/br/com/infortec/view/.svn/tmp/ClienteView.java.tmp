private void comboTipoItemStateChanged(java.awt.event.ItemEvent evt) {//GEN-FIRST:event_comboTipoItemStateChanged
    if ("Física".equals((String) comboTipo.getSelectedItem())) {//pessoa física
        txtCNPJ.setVisible(false);
        cnpjjLabel.setVisible(false);
        cpfLabel.setVisible(true);
        txtCPF.setVisible(true);
        nascimentoClienteLabel.setVisible(true);
        orgaoRgClienteLabel.setVisible(true);
        txtOrgao.setVisible(true);
        txtNascimento.setVisible(true);
        txtRG.setVisible(true);
        rgClienteLabel.setVisible(true);

    } else if ("Jurídica".equals((String) comboTipo.getSelectedItem())) {//pessoa jurídica
        txtCNPJ.setVisible(true);
        cnpjjLabel.setVisible(true);
        cpfLabel.setVisible(false);
        txtCPF.setVisible(false);
        nascimentoClienteLabel.setVisible(false);
        orgaoRgClienteLabel.setVisible(false);
        txtOrgao.setVisible(false);
        txtNascimento.setVisible(false);
        txtRG.setVisible(false);
        rgClienteLabel.setVisible(false);

    } else if ("".equals((String) comboTipo.getSelectedItem())) {
        txtCNPJ.setVisible(false);
        cnpjjLabel.setVisible(false);
        cpfLabel.setVisible(false);
        txtCPF.setVisible(false);
    }
}//GEN-LAST:event_comboTipoItemStateChanged
