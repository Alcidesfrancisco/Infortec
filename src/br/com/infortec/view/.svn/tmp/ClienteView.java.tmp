    private void btSalvarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btSalvarActionPerformed
        if (cont % 2 == 0) {
            String nome = txtNome.getText();
            String cpf = txtCPF.getText();
            String rg = txtRG.getText();
            String orgao = txtOrgao.getText();
            String nascimento = txtNascimento.getText();
            String profissao = txtProfissao.getText();
            String empresa = txtEmpresa.getText();
            String foneEmpresa = txtFoneEmpresa.getText();
            String tipo = (String) comboTipo.getSelectedItem();
            String cnpj = txtCNPJ.getText();
            String referencia = txtReferencia.getText();
            String Email = txtEmail.getText();
            Cliente cliente = new Cliente(nome, cpf, rg, orgao, nascimento, profissao, empresa, foneEmpresa, tipo, cnpj, referencia, referencia, Email);
            int idForma = insereCliente.inserirCliente(nome, cpf, rg, orgao, nascimento, profissao, empresa, foneEmpresa, tipo, cnpj, referencia, referencia, Email);

            listaEditaCliente.add(cliente);
            table.setModel(tabelaModel(listaEditaCliente));
            int row = table.getRowCount() - 1;
            if (table.getRowCount() >= 1) {
                table.setRowSelectionInterval(row, row);
            }
            table.scrollRectToVisible(table.getCellRect(table.getRowCount() + 1, 0, true));

            txtNome.setText("");
            txtCNPJ.setText("");
            txtCPF.setText("");
            txtEmail.setText("");
            txtEmpresa.setText("");
            txtFoneEmpresa.setText("");
            txtFoneReferencia.setText("");
            txtNascimento.setText("");
            txtOrgao.setText("");
            txtProfissao.setText("");
            txtRG.setText("");
            txtReferencia.setText("");
        } else {
            String alterNome = txtNome.getText();
            String alterCPF = txtCPF.getText();
            String alterRG = txtRG.getText();
            String alterOrgao = txtOrgao.getText();
            String alterNascimento = (txtNascimento.getText());
            String alterProfissao = txtProfissao.getText();
            String alterEmpresa = txtEmpresa.getText();
            String alterFoneEmpresa = txtFoneEmpresa.getText();
            String alterTipo = (String) comboTipo.getSelectedItem();
            String alterCNPJ = txtCNPJ.getText();
            String alterFoneReferencia = txtReferencia.getText();
            String alterEmail = txtEmail.getText();

            Cliente clienteAlterar = listaEditaCliente.get(table.getSelectedRow());

            clienteAlterar.setNome(alterNome);
            clienteAlterar.setCpf(alterCPF);
            clienteAlterar.setRg(alterRG);
            clienteAlterar.setOrgao(alterOrgao);
            clienteAlterar.setDataNascimento(alterNascimento);
            clienteAlterar.setProfissao(alterProfissao);
            clienteAlterar.setEmpresa(alterEmpresa);
            clienteAlterar.setFoneEmpresa(alterEmpresa);
            clienteAlterar.setTipoCliente(alterTipo);
            clienteAlterar.setCnpj(alterCNPJ);
            clienteAlterar.setFoneReferencia(alterFoneReferencia);
            clienteAlterar.setEmail(alterEmail);

            clienteDAO.alteraCliente(clienteAlterar, clienteAlterar.getCodCliente());

            table.setModel(tabelaModel(listaEditaCliente));

            txtNome.setText("");
            txtCNPJ.setText("");
            txtCPF.setText("");
            txtEmail.setText("");
            txtEmpresa.setText("");
            txtFoneEmpresa.setText("");
            txtFoneReferencia.setText("");
            txtNascimento.setText("");
            txtOrgao.setText("");
            txtProfissao.setText("");
            txtRG.setText("");
            txtReferencia.setText("");

        }
    }//GEN-LAST:event_btSalvarActionPerformed

private void btEditarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btEditarActionPerformed
    if (cont % 2 == 0) {      //editar linha
        txtNome.setEnabled(true);
        txtCNPJ.setEnabled(true);
        txtCPF.setEnabled(true);
        txtEmail.setEnabled(true);
        txtEmpresa.setEnabled(true);
        txtFoneEmpresa.setEnabled(true);
        txtFoneReferencia.setEnabled(true);
        txtNascimento.setEnabled(true);
        txtOrgao.setEnabled(true);
        txtProfissao.setEnabled(true);
        txtRG.setEnabled(true);
        txtReferencia.setEnabled(true);
        btEditar.setText("Cancelar Edição");
        tabPanel.setSelectedIndex(1);
        txtNome.requestFocus();
        cont++;
        editarLinha();

    } else {                //cancelar edição
        txtNome.setEnabled(false);
        txtCNPJ.setEnabled(false);
        txtCPF.setEnabled(false);
        txtEmail.setEnabled(false);
        txtEmpresa.setEnabled(false);
        txtFoneEmpresa.setEnabled(false);
        txtFoneReferencia.setEnabled(false);
        txtNascimento.setEnabled(false);
        txtOrgao.setEnabled(false);
        txtProfissao.setEnabled(false);
        txtRG.setEnabled(false);
        txtReferencia.setEnabled(false);
        txtNome.requestFocus();
        btEditar.setText("Editar");
        cont++;
    }

}//GEN-LAST:event_btEditarActionPerformed

